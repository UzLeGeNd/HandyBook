<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.AllBooksFragment">
    <View
        android:id="@+id/roman_back"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/baseline_keyboard_backspace_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </View>

    <View
        android:id="@+id/roman_acc"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/baseline_account_circle_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent">
    </View>

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Romanlar"
        app:layout_constraintEnd_toStartOf="@+id/roman_acc"
        app:layout_constraintStart_toEndOf="@id/roman_back"
        app:layout_constraintTop_toTopOf="@id/roman_back"
        app:layout_constraintBottom_toBottomOf="@id/roman_back"
        android:textSize="22dp"
        android:textColor="@color/black"
        tools:layout_editor_absoluteY="20dp" />



</androidx.constraintlayout.widget.ConstraintLayout>